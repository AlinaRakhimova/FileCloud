<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="310.0" prefWidth="736.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.rakhimova.controller.Controller">
   <children>
      <ScrollPane prefHeight="241.0" prefWidth="620.0" GridPane.rowIndex="1">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="240.0" prefWidth="679.0">
               <children>
                  <TreeView id="treeViewFiles" fx:id="treeViewFiles" onMouseClicked="#changeFile" prefHeight="241.0" prefWidth="679.0" />
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="30.0" prefWidth="693.0">
