            <Button id="buttonDeleteFolder" mnemonicParsing="false" onMouseClicked="#deleteFolder" text="Delete folder" />
            <Button id="buttonAddFile" alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onMouseClicked="#addFile" text="Add file" />
            <Button mnemonicParsing="false" onMouseClicked="#deleteFile" text="Delete file" />
