            <TextField id="textFieldName" fx:id="textFieldName" prefHeight="25.0" prefWidth="190.0" />
            <ToggleButton id="buttonRemote" fx:id="buttonRemote" mnemonicParsing="false" onMouseClicked="#changeLabelRemote" text="Local" textAlignment="CENTER" />
         </children>
         <opaqueInsets>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </opaqueInsets>
      </HBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
